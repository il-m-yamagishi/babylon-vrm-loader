{
    "$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
    "organizeImports": {
        "enabled": true
    },
    "formatter": {
        "enabled": true,
        "indentSize": 4,
        "indentStyle": "space",
        "indentWidth": 4,
        "lineEnding": "crlf",
        "lineWidth": 200
    },
    "linter": {
        "enabled": true,
        "rules": {
            "recommended": true
        }
    }
}
